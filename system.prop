#
# system.prop for Huawei U8150
#

# RIL properties
rild.libargs=-d /dev/smd0

# zRam properties
ro.zram.default=0

# Audio properties
persist.sys.fm-attn=0
persist.sys.headset-attn=0
persist.sys.speaker-attn=6

# SD card readahead buffer (U8150Parts)
persist.sys.read_ahead_kb=0

# Background process limit (U8150Parts)
persist.sys.processlimit=2
